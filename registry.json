      "description": "Breitling Button Component",
      "registryDependencies": ["theme"],
      "files": [
        {
          "path": "components/ui/button.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
      "name": "card",
      "type": "registry:ui",
      "title": "Card",
      "description": "Breitling Card Component",
      "registryDependencies": ["theme"],
      "files": [
        {
          "path": "components/ui/card.tsx",
          "type": "registry:component"
        }
      ]
    },
    {
      "name": "dropdown-menu",
      "type": "registry:ui",
      "title": "Dropdown Menu",
      "description": "Breitling Dropdown Menu Component",
      "registryDependencies": ["theme"],
      "files": [
        {
          "path": "components/ui/dropdown-menu.tsx",
          "type": "registry:component"
        }
